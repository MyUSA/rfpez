.project-list
  list-style none
  font-size 1.4em
  li
    mb 1.3em

.show-my-projects-btn
  fs .65em
  fw normal
  pl 12px

.new-project-btn
  ml 20px

.projects-table
  thead
    tr
      th.project-title
